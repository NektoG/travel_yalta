<!DOCTYPE html>
<html lang="ru">
	<head>
		<meta charset="UTF-8">
		<meta http-equiv="content-language" content="ru">
		<meta name="description" content="<%= introduction[0]['Description'] %>">
		<meta name="keywords" content="<%= real_keywords %>">

		<meta name="application-name" content="<%= real_name_project %>">
		<meta name="generator" content="Node JS">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title><% for(var i=0; i < name_cite.length; i++) { %><%if(name_cite[i]['Cite'] === type_cite){%><%= name_cite[i]['Name'] %> <% } %><% } %><%= real_name_project %></title>
		<link rel="shortcut icon" href="<%= image_favicon %><%= favicon[0]['Link'] %>">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
		<link rel="stylesheet" href="https://unpkg.com/swiper@8/swiper-bundle.min.css"/>
		<link rel="stylesheet" href="../Source/default/css/resource.css">
		<link rel="stylesheet" href="../Source/Elements/Header/css/resource.css">
		<link rel="stylesheet" href="../Source/Service_Additionally/css/resource.css">
		<link rel="stylesheet" href="../Source/Elements/Footer/css/resource.css">
	</head>
	<body>
		<%- include ('Elements/header.ejs'); %>
	  <div id="Main_Base">
	    <div class="Base g-0">
				<%if(introduction[0]['Topic'] != '' || introduction[0]['Description'] != ''){%>
	    		<section id="Introduction">
	    			<div class="anim_active">
							<%if(introduction[0]['Topic'] != ''){%>
							<h3><%= introduction[0]['Topic'] %></h3>
							<%}%>
							<%if(introduction[0]['Description'] != ''){%>
							<h5><%= introduction[0]['Description'] %></h5>
							<%}%>
						</div>
				</section>
				<%}%>
				<% if(data_card != null && data_card != undefined && data_content != null && data_content != undefined) { %>
				<section id="Content">
					<div class="mySwiper">
			      <div class="swiper-wrapper">
							<% for(var i = (data_card.length-1); i >= 0; i--) { %>
			        <div class="swiper-slide">
								<a href="<%= link %><%= data_card[i]['ID'] %>">
					      	<div class="card _anim_elements kek">
									  <div class="image">
											<%if(data_card[i]['Text'] != ''){%>
											<div class="text">
												<div>
													<h6><%= data_card[i]['Text'] %></h6>
												</div>
											</div>
											<%}%>
											<% if(data_card[i]['Price_1'] != '' || data_card[i]['Price_2'] != '' || data_card[i]['Price_3'] != '' || data_card[i]['Price_4'] != '' || data_card[i]['Price_5'] != '') { %>
											<div class="price">
												<%if(data_card[i]['Price_1'] != ''){%>
												<div class="price_el">
													<h4 class="text-light"><%= data_card[i]['Price_1']%>&#8381;</h4>
												</div>
												<%}%>
												<%if(data_card[i]['Price_2'] != ''){%>
												<div class="price_el">
													<h4 class="text-light"><%= data_card[i]['Price_2']%>&#8381;</h4>
												</div>
												<%}%>
												<%if(data_card[i]['Price_3'] != ''){%>
												<div class="price_el">
													<h4 class="text-light"><%= data_card[i]['Price_3']%>&#8381;</h4>
												</div>
												<%}%>
												<%if(data_card[i]['Price_4'] != ''){%>
												<div class="price_el">
													<h4 class="text-light"><%= data_card[i]['Price_4']%>&#8381;</h4>
												</div>
												<%}%>
												<%if(data_card[i]['Price_5'] != ''){%>
												<div class="price_el">
													<h4 class="text-light"><%= data_card[i]['Price_5']%>&#8381;</h4>
												</div>
												<%}%>
											</div>
											<% } %>
											<div class="Slider">
												<div class="swiper">
												  <div class="swiper-wrapper">
														<% if(data_type_info[i][0] === 0 || data_type_info[i][1] === 0 || data_type_info[i][2] === 0 || data_type_info[i][3] === 0 || data_type_info[i][4] === 0){ %>
															<% for(let j = 1; j <= 5; j++) { %>
															<% if(data_type_info[i][(j-1)] === 0){ %>
																<% if(data_content[i][`ะก_${j}`] != '' && data_content[i][`ะก_${j}`] != null) { %>
																<div class="swiper-slide">
																	<img src="<% if(data_type_img[i][(j-1)] === 0){ %><%= img_link %><%= data_card[i]['ID'] %>/<% } %><%= data_content[i][`ะก_${j}`]%>" />
																</div>
															<% } } } %>
														<% } else {%>
															<div class="swiper-slide">
																<img src="../Image/Default/for_slider/1.jpg" />
															</div>
															<div class="swiper-slide">
																<img src="../Image/Default/for_slider/2.jpg" />
															</div>
															<div class="swiper-slide">
																<img src="../Image/Default/for_slider/3.jpg" />
															</div>
														<% } %>
												  </div>
											  	<div class="swiper-pagination"></div>
												</div>
											</div>
									</div>
									<%if(data_card[i]['Topic'] != '' && data_card[i]['Desc_2'] != ''){%>
								  <div class="card-body">
										<%if(data_card[i]['Topic'] != ''){%>
								  	<h5><%= data_card[i]['Topic'] %></h5>
										<% } %>
										<%if(data_card[i]['Desc_2'] != ''){%>
								    <p class="card-text"><%= data_card[i]['Desc_2'] %></p>
										<% } %>
								  </div>
									<% } %>
								</div>
								</a>
							</div>
							<% } %>
			      </div>
			    </div>
		    </section>
				<% } %>
		    <section id="PSS" class="px-2">
	    			<div class="_anim_elements kek">
	    			<hr class="hr_top">
						<h2>P.S.</h2>
						<h4><%= no_find[0]['PS'] %></h4>
						<hr class="hr_bottom">
	    			</div>
				</section>

				<%if(no_find[0]['Topic_Main'] != '' && no_find[0]['Description_Main'] != '' && no_find[0]['Topic_Two'] != '' && no_find[0]['Description_Two'] != ''){%>
	      	<section id="Offer" class="px-2">
	      		<div class="_anim_elements kek">
							<%if(no_find[0]['Topic_Main'] != ''){%>
	      			<h4><%= no_find[0]['Topic_Main'] %></h4>
							<% } %>
							<%if(no_find[0]['Description_Main'] != ''){%>
							<h5><%= no_find[0]['Description_Main'] %></h5>
							<% } %>
							<%if(no_find[0]['Topic_Main'] != '' && no_find[0]['Description_Main'] != ''){%>
							<hr>
							<% } %>
							<%if(no_find[0]['Topic_Two'] != ''){%>
	      			<h4><%= no_find[0]['Topic_Two'] %></h4>
							<% } %>
							<%if(no_find[0]['Description_Two'] != ''){%>
							<h5><%= no_find[0]['Description_Two'] %></h5>
							<% } %>
	      		</div>
					</section>
				<% } %>
	    </div>
	  </div>
	<%- include ('Elements/footer.ejs'); %>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
	<script src="https://unpkg.com/swiper@8/swiper-bundle.min.js"></script>
	<script src="../Source/Service_Additionally/js/for_slider.js"></script>
	<script src="../Source/default/js/for_anim_move_up.js"></script>
	<script src="../Source/Service_Additionally/js/anim_price_el.js"></script>
	</body>
</html>
